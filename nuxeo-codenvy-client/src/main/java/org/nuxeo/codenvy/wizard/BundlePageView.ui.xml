<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
  <g:DockLayoutPanel>
    <g:north size="25">
      <g:Label text="Page 1"/>
    </g:north>
    <g:north size="25">
      <g:Label text="Next page is:"/>
    </g:north>
    <g:north size="25">
      <g:RadioButton ui:field="page2" name="page" text="Page 2"/>
    </g:north>
    <g:north size="25">
      <g:CheckBox ui:field="page4" text="show page 4"/>
    </g:north>
  </g:DockLayoutPanel>
</ui:UiBinder>